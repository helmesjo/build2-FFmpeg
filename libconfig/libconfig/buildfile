using autoconf

define asm: file
asm{*}: extension = asm

define groups: file
groups{*}: extension = groups

intf_libs = # Interface dependencies.
impl_libs = # Implementation dependencies.

cpu = [string] $c.target.cpu
tgt_win32 = [bool] ($c.target.system == 'win32-msvc')
tgt_macos = [bool] ($c.target.system == 'darwin')

./: lib{config}: h{autoconf config config_components} asm{config.asm} $intf_libs

c{print_defines}: h{autoconf}
{{
  s = [path] "$src_base/config.h.in"
  print = [string] "#pragma message"

  diag gen $>

  defs = [string_set]
  file = [strings]

  file += '#include "autoconf.h"'
  file += '#define STRINGIFY(x) #x'
  file += '#define TOSTRING(x) STRINGIFY(x)'
  file += '#define VALUE(v) v'

  file += ""
  cat $s | for -n line
    if ($regex.find_match($line, '[ ]*#[ ]*?(define|undef)[ ]+.*$'))
      c = $regex.apply($line, '[ ]*#[ ]*?(define|undef)[ ]+([A-Z0-9_]+).*$', '\2', format_no_copy)
      if (!$empty($c) && $c != 'FFMPEG_CONFIG_H' && !$defs[$c])
        defs += $c
        tmp = [string] 'TOSTRING({#define "DEF"=VALUE(DEF)})'
        tmp = $string.replace($tmp, 'DEF', "$c")
        file += "#ifndef $c"
        file += "  #define $c 0"
        file += "#endif"
        file += "$print\($tmp\)"
      end
    end
  end
  file += "$print\(\"#endif\"\)"
  file = $regex.apply($file, '^(.*)$', "\1\n", format_no_copy)

  echo '// generated' >$path($>)
  echo $file >>$path($>)
}}

h{config}: c{print_defines}
{{
  s = $path($<)
  n = $path.leaf($s)
  o = $path.base($path($>)).$extension($>).obj
  r = $(o).cli

  diag configure ($>)

  if ($c.target.system == 'win32-msvc')
    echo -nologo $c.poptions $cc.poptions $c.coptions $c.mode -c $s -Fo$o >$r &$r &$o
    $effect($c.path) "@$r" | set tmp [string_set]
  else
    $effect($c.path) $c.poptions $cc.poptions $c.coptions $c.mode -E $s 2>! \
                     | set tmp [string_set]
    # quotes are printed as unicode with clang
    tmp = $regex.replace($tmp, '\\042', '"')
  end

  tmp = $regex.apply($tmp, '\{(#[a-zA-Z]+) [\\"]{1,2}([A-Za-z0-9_]+)[\\"]{1,2}=(.*)\}', "\1 \2 \3\n", format_no_copy)
  tmp = $regex.replace($tmp, '^\s+((?!#).*$)', '//\1')
  tmp = $regex.replace($tmp, '\\\"', '"')

  echo '// generated' >$path($>)
  echo '#ifndef FFMPEG_CONFIG_H' >>$path($>)
  echo '#define FFMPEG_CONFIG_H' >>$path($>)
  echo '' >>$path($>)
  echo "$tmp" >>$path($>)
  echo '#endif' >>$path($>)
}}

asm{config.asm}: h{config}
{{
  s = $path($<)

  diag configure ($>)

  cat $s | set tmp [string_set]
  tmp = $regex.replace($tmp, '#', '%')
  tmp = $regex.replace($tmp, '^//', '; ')

  echo "$tmp" >$path($>)
}}

