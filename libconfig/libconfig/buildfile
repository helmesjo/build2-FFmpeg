# Build options.
#
out_pfx = [dir_path] $out_root/
src_pfx = [dir_path] $src_root/libconfig/

c.poptions += "-I$out_pfx" "-I$out_pfx/libconfig/" "-I$src_pfx"

liba{config}:
{
  c.poptions += -DCONFIG_STATIC
}
libs{config}:
{
  c.poptions += -DCONFIG_SHARED
}

switch $c.target.system
{
  case 'win32-msvc'
  {
    lib{config}: c.export.loptions +=  -LARGEADDRESSAWARE
  }
}

# Export options.
#
lib{config}:
{
  c.export.poptions += "-I$out_pfx"  "-I$out_pfx/libconfig/" "-I$src_pfx" -DHAVE_AV_CONFIG_H
  c.export.libs += $intf_libs
  libconfig.disable_asm                   = $libconfig.disable_asm
  libconfig.disable_altivec               = $libconfig.disable_altivec
  libconfig.disable_vsx                   = $libconfig.disable_vsx
  libconfig.disable_power8                = $libconfig.disable_power8
