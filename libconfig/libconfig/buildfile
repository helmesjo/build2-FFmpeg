# Build options.
#
out_pfx = [dir_path] $out_root/
src_pfx = [dir_path] $src_root/libconfig/

c.poptions += "-I$out_pfx" "-I$out_pfx/libconfig/" "-I$src_pfx"

cflags    = [strings]
pflags    = [strings]
spflags   = [strings]
lflags    = [strings]
sys_libs  = [strings]
switch $c.target.system
{
  case 'win32-msvc'
  {
    cflags = -wd4018 \
             -wd4133 \
             -wd4146 \
             -wd4244 \
             -wd4305 \
             -wd4333 \
             -wd4554
    lflags = -LARGEADDRESSAWARE
  }
  default
  {
    cflags = -Wno-deprecated-declarations \
             -Wno-deprecated-pragma \
             -Wno-implicit-const-int-float-conversion \
             -Wno-implicit-function-declaration \
             -Wno-incompatible-pointer-types \
             -Wno-parentheses \
             -Wno-pointer-sign \
             -Wno-shift-op-parentheses \
             -Wno-switch
    pflags = -D_POSIX_SOURCE \
             -D_ISOC11_SOURCE -D__STDC_CONSTANT_MACROS -D_POSIX_C_SOURCE=200112 -D_XOPEN_SOURCE=600
    spflags = -DCONFIG_PIC=1 -DPIC=1
    lflags = -Wl,-Bsymbolic
    sys_libs = -lm

    if($c.id == 'clang-apple')
    {
      spflags += -Wl,-dynamic,-search_paths_first
    }
  }
}

c.libs += $sys_libs

liba{config}:
{
  c.poptions += -DCONFIG_STATIC
}
libs{config}:
{
  c.poptions += -DCONFIG_SHARED
}

# Export options.
#
lib{config}:
{
  c.export.poptions += "-I$out_pfx"  "-I$out_pfx/libconfig/" "-I$src_pfx" -DHAVE_AV_CONFIG_H $pflags
  # c.export.coptions is ignored for binless targets, so add it to poptions instead
  c.export.poptions += $cflags
  c.export.loptions += $lflags
  c.export.libs     += $intf_libs $sys_libs
}

liba{config}:
{
  c.export.poptions += -DCONFIG_STATIC
}
libs{config}:
{
  c.export.poptions += -DCONFIG_SHARED $spflags
}

# For pre-releases use the complete version to make sure they cannot
# be used in place of another pre-release or the final version. See
# the version module for details on the version.* variable values.
#
if $version.pre_release
  lib{config}: bin.lib.version = "-$version.project_id"
else
  lib{config}: bin.lib.version = "-$version.major.$version.minor"

# Install recreating subdirectories.
#
h{*}:
{
  install         = include/
  install.subdirs = true
}
