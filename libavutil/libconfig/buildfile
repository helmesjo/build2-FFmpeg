  CONFIG_FD_PROTOCOL                 = ($config.libavutil.fd_protocol ? 1 : 0)
  CONFIG_FFRTMPCRYPT_PROTOCOL        = ($config.libavutil.ffrtmpcrypt_protocol ? 1 : 0)
  CONFIG_FFRTMPHTTP_PROTOCOL         = ($config.libavutil.ffrtmphttp_protocol ? 1 : 0)
  CONFIG_FILE_PROTOCOL               = ($config.libavutil.file_protocol ? 1 : 0)
  CONFIG_FTP_PROTOCOL                = ($config.libavutil.ftp_protocol ? 1 : 0)
  CONFIG_GOPHER_PROTOCOL             = ($config.libavutil.gopher_protocol ? 1 : 0)
  CONFIG_GOPHERS_PROTOCOL            = ($config.libavutil.gophers_protocol ? 1 : 0)
  CONFIG_HLS_PROTOCOL                = ($config.libavutil.hls_protocol ? 1 : 0)
  CONFIG_HTTP_PROTOCOL               = ($config.libavutil.http_protocol ? 1 : 0)
  CONFIG_HTTPPROXY_PROTOCOL          = ($config.libavutil.httpproxy_protocol ? 1 : 0)
  CONFIG_HTTPS_PROTOCOL              = ($config.libavutil.https_protocol ? 1 : 0)
  CONFIG_ICECAST_PROTOCOL            = ($config.libavutil.icecast_protocol ? 1 : 0)
  CONFIG_MMSH_PROTOCOL               = ($config.libavutil.mmsh_protocol ? 1 : 0)
  CONFIG_MMST_PROTOCOL               = ($config.libavutil.mmst_protocol ? 1 : 0)
  CONFIG_MD5_PROTOCOL                = ($config.libavutil.md5_protocol ? 1 : 0)
  CONFIG_PIPE_PROTOCOL               = ($config.libavutil.pipe_protocol ? 1 : 0)
  CONFIG_PROMPEG_PROTOCOL            = ($config.libavutil.prompeg_protocol ? 1 : 0)
  CONFIG_RTMP_PROTOCOL               = ($config.libavutil.rtmp_protocol ? 1 : 0)
  CONFIG_RTMPE_PROTOCOL              = ($config.libavutil.rtmpe_protocol ? 1 : 0)
  CONFIG_RTMPS_PROTOCOL              = ($config.libavutil.rtmps_protocol ? 1 : 0)
  CONFIG_RTMPT_PROTOCOL              = ($config.libavutil.rtmpt_protocol ? 1 : 0)
  CONFIG_RTMPTE_PROTOCOL             = ($config.libavutil.rtmpte_protocol ? 1 : 0)
  CONFIG_RTMPTS_PROTOCOL             = ($config.libavutil.rtmpts_protocol ? 1 : 0)
  CONFIG_RTP_PROTOCOL                = ($config.libavutil.rtp_protocol ? 1 : 0)
  CONFIG_SCTP_PROTOCOL               = ($config.libavutil.sctp_protocol ? 1 : 0)
  CONFIG_SRTP_PROTOCOL               = ($config.libavutil.srtp_protocol ? 1 : 0)
  CONFIG_SUBFILE_PROTOCOL            = ($config.libavutil.subfile_protocol ? 1 : 0)
  CONFIG_TEE_PROTOCOL                = ($config.libavutil.tee_protocol ? 1 : 0)
  CONFIG_TCP_PROTOCOL                = ($config.libavutil.tcp_protocol ? 1 : 0)
  CONFIG_TLS_PROTOCOL                = ($config.libavutil.tls_protocol ? 1 : 0)
  CONFIG_UDP_PROTOCOL                = ($config.libavutil.udp_protocol ? 1 : 0)
  CONFIG_UDPLITE_PROTOCOL            = ($config.libavutil.udplite_protocol ? 1 : 0)
  CONFIG_UNIX_PROTOCOL               = ($config.libavutil.unix_protocol ? 1 : 0)
  CONFIG_LIBAMQP_PROTOCOL            = ($config.libavutil.libamqp_protocol ? 1 : 0)
  CONFIG_LIBRIST_PROTOCOL            = ($config.libavutil.librist_protocol ? 1 : 0)
  CONFIG_LIBRTMP_PROTOCOL            = ($config.libavutil.librtmp_protocol ? 1 : 0)
  CONFIG_LIBRTMPE_PROTOCOL           = ($config.libavutil.librtmpe_protocol ? 1 : 0)
  CONFIG_LIBRTMPS_PROTOCOL           = ($config.libavutil.librtmps_protocol ? 1 : 0)
  CONFIG_LIBRTMPT_PROTOCOL           = ($config.libavutil.librtmpt_protocol ? 1 : 0)
  CONFIG_LIBRTMPTE_PROTOCOL          = ($config.libavutil.librtmpte_protocol ? 1 : 0)
  CONFIG_LIBSRT_PROTOCOL             = ($config.libavutil.libsrt_protocol ? 1 : 0)
  CONFIG_LIBSSH_PROTOCOL             = ($config.libavutil.libssh_protocol ? 1 : 0)
  CONFIG_LIBSMBCLIENT_PROTOCOL       = ($config.libavutil.libsmbclient_protocol ? 1 : 0)
  CONFIG_LIBZMQ_PROTOCOL             = ($config.libavutil.libzmq_protocol ? 1 : 0)
  CONFIG_IPFS_GATEWAY_PROTOCOL       = ($config.libavutil.ipfs_gateway_protocol ? 1 : 0)
  CONFIG_IPNS_GATEWAY_PROTOCOL       = ($config.libavutil.ipns_gateway_protocol ? 1 : 0)
}

# Build options.
#
out_pfx = [dir_path] $out_root/libconfig/
src_pfx = [dir_path] $src_root/libconfig/

c.poptions =+ "-I$out_pfx" "-I$src_pfx"

# Export options.
#
lib{config}:
{
  c.export.poptions += "-I$out_pfx" "-I$src_pfx" -DHAVE_AV_CONFIG_H
}
liba{config}:
{
  c.export.poptions += -DCONFIG_STATIC
}
libs{config}:
{
  c.export.poptions += -DCONFIG_SHARED
}

# For pre-releases use the complete version to make sure they cannot
# be used in place of another pre-release or the final version. See
# the version module for details on the version.* variable values.
#
if $version.pre_release
  lib{config}: bin.lib.version = "-$version.project_id"
else
  lib{config}: bin.lib.version = "-$version.major.$version.minor"

# Install recreating subdirectories.
#
h{*}:
{
  install         = include/
  install.subdirs = true
}
