using autoconf

cpu = [string] $c.target.cpu
tgt_win32 = [bool] ($c.target.system == 'win32-msvc')
tgt_macos = [bool] ($c.target.system == 'darwin')

./: lib{config}: h{config config_components} file{config.asm}: update = match

file{config.asm}: h{config}
{{
  diag configure $>
  cat $path($<) | sed -n -e 's/^#define(.+)$/%define \1/p' >$path($>)
}}

./: h{config}

./: file{hello}: h{config}
% update clean
recipe c++ print_defines.cxx

h{config}: in{config}
{
  FFMPEG_CONFIGURATION                 = "\"$c.target.system\""
  FFMPEG_LICENSE                       = "\"LGPL version 2.1 or later\""
  SWS_MAX_FILTER_SIZE                  = 256

  OS_NAME                              = ($c.target.class)
  ARCH_X86                             = ($regex.match($cpu, 'i[3-6]86') ? 1 : 0)
  ARCH_X86_32                          = ($regex.match($cpu, 'i[3-6]86') ? 1 : 0)
  ARCH_X86_64                          = ($regex.match($cpu, 'x86_64') ? 1 : 0)
  ARCH_AARCH64                         = ($regex.match($cpu, 'aarch64') ? 1 : 0)
